{
  "name": "gremista",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "runIos": "yarn && cd ios && pod install && cd .. && yarn ios",
    "runAndroid": "yarn && cd android && ./gradlew clean && cd .. && yarn android",
    "test": "jest",
    "lint": "eslint .",
    "generate": "npx react-native generate-bootsplash ./src/assets/original_icon.png --background-color=2EE59D   --logo-width=100 --assets-path=./src/assets --flavor=main",
    "assembleDebug": "npm-run-all assembleDebug:*",
    "assembleDebug:unlink": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "assembleDebug:clean": "cd android && ./gradlew clean",
    "assembleDebug:assemble": "cd android && ./gradlew assembleDebug"
  },
  "dependencies": {
    "@react-native-admob/admob": "^2.0.0",
    "@react-native-async-storage/async-storage": "^1.15.16",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/drawer": "5.x",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "axios": "^0.24.0",
    "i18next": "^21.6.14",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react": "17.0.1",
    "react-i18next": "^11.16.2",
    "react-native": "0.64.2",
    "react-native-admob-native-ads": "^0.5.0",
    "react-native-bootsplash": "3.0.0",
    "react-native-dimension": "^1.0.6",
    "react-native-elements": "^3.4.2",
    "react-native-fast-image": "^8.5.11",
    "react-native-flash-message": "^0.1.23",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-iap": "7.3.0",
    "react-native-indicators": "^0.17.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal": "^11.10.0",
    "react-native-progress": "^5.0.0",
    "react-native-push-notification": "^8.1.1",
    "react-native-reanimated": "^1.13.2",
    "react-native-render-html": "^6.3.1",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.15.0",
    "react-native-share": "^7.3.2",
    "react-native-vector-icons": "^9.0.0",
    "react-native-webview": "11.15.1",
    "react-native-youtube": "^2.0.2",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "17.0.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
